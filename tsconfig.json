{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/_layout.jsx",
    "app/(tabs)/home.jsx",
    "app/(tabs)/profile.jsx",
    "app/(tabs)/friend.jsx",
    "app/message.tsx",
    "app/chat.jsx"
  ]
}
